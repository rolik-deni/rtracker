{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tvnetwork</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tvNetwork.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ tvNetwork.title }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tvnetwork_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tvnetwork_edit', { 'id': tvNetwork.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
